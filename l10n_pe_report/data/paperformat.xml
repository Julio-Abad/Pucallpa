<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="paperformat_cpe" model="report.paperformat">
            <field name="name">Cabal Formato CPE</field>
            <field eval="True" name="default"/>
            <field name="format">A4</field>
            <!-- <field name="page_height">25</field>
            <field name="page_width">50</field> -->
            <field name="orientation">Portrait</field>
            <field name="margin_top">50</field>
            <field name="margin_bottom">25</field>
            <field name="margin_left">4</field>
            <field name="margin_right">4</field>
            <field eval="False" name="header_line"/>
            <field name="header_spacing">45</field>
            <!-- <field name="dpi">150</field> -->
        </record>

    </data>
</odoo>
